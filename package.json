{
  "name": "react-user-table",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:eslint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --log-level error --write src",
    "lint:types": "tsc --noEmit",
    "lint": "npm run lint:types && npm run lint:eslint:fix && npm run lint:prettier",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@tanstack/react-query": "^5.20.5",
    "axios": "^1.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@commitlint/cz-commitlint": "^18.6.1",
    "@conarti/eslint-plugin-feature-sliced": "^1.0.5",
    "@faker-js/faker": "^8.4.1",
    "@mswjs/data": "^0.16.1",
    "@storybook/addon-essentials": "^7.6.15",
    "@storybook/addon-interactions": "^7.6.15",
    "@storybook/addon-links": "^7.6.15",
    "@storybook/blocks": "^7.6.15",
    "@storybook/react": "^7.6.15",
    "@storybook/react-vite": "^7.6.15",
    "@storybook/test": "^7.6.15",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.0",
    "msw-storybook-addon": "2.0.0-beta.1",
    "prettier": "^3.2.5",
    "sass": "^1.70.0",
    "storybook": "^7.6.15",
    "typescript": "^5.3.3",
    "vite": "^5.1.2",
    "vitest": "^1.2.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
